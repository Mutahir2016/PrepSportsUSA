	<key>UIAppFonts</key>
	<array>
		<string>IBMPlexSans-Medium.ttf</string>
		<string>IBMPlexSans-Bold.ttf</string>
		<string>IBMPlexSans-SemiBold.ttf</string>
		<string>IBMPlexSans-Regular.ttf</string>
	</array>
